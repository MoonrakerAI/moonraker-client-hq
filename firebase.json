{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "us-west1",
      "runtime": "nodejs20"
    }
  },
  "firestore": [
    {
      "database": "moonraker-client-hq",
      "rules": "firestore.rules",
      "indexes": "firestore.indexes.json"
    }
  ]
}